package task4;

public class IndentObserver implements Observer<Entry> {
	
	private int countEntries(Entry e) {
        // 木構造eの中のEntryの数を返す．
	int count = 1;
	for (Entry child : e.getChildren()) {
	    count += countEntries(child);
	}
	return count;
    }
	
	
    private void printEntries(int depth, Entry e) {
       
    	
    }
    
    public void update(int depth, Entry e) {
	System.out.println(printEntries(depth, e));
    }
}