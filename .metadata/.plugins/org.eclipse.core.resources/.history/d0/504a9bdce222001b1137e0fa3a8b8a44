package task4;

public abstract class FileSystem {
    private static FileSystem instance = null;
    public static FileSystem getInstance() {
      // 唯一のインスタンスを(必要なら生成して)返す．
    	public final Directory createDirectory(String name){
    		Directory dir = createDirectory(name);
    		return instance;
    	}
    	public final Directory createDirectory(String name){
    		Directory instance = createDirectory(name);
    	}
    	
    	
    }
    public abstract Directory createDirectory(String name);
    public abstract File createFile(String name);
}