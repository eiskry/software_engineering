package task4;

import java.util.List;
import java.util.LinkedList;

public class Directory implements Subject<Entry> {
	// 観察者リストを用意する
    private static List<Directory> observerList = new LinkedList<Directory>();
    void addObserver(Directory o) {
    	observerList.add(o);
    }
    void notifyObservers(o) {
    	for (Observer observer : observerList) {
    		observer.update(this);
    	}
    }
}